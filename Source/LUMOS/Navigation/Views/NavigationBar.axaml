<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="LUMOS.Navigation.Views.NavigationBar">
	<Panel HorizontalAlignment="Stretch">
		<DockPanel Name="Menu">
			<Image Name="Previous" Width="48" Height="48" Margin="15,0,0,0" Source="/Assets/navigation-previous.png" DockPanel.Dock="Left" />
			<Image Name="Search" Width="48" Height="48" Margin="0,0,15,0" Source="/Assets/navigation-search.png" DockPanel.Dock="Right" />
			<Image Name="Home" Width="48" Height="48" Margin="15,0,15,0" Source="/Assets/navigation-home.png" />
		</DockPanel>
	</Panel>
</UserControl>
